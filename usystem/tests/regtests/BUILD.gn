import("//build/app.gni")

app_executable("nameitests") {
    include_dirs = [ "include" ]
    package_name = "nameitests"

    output_name = "nameitests"

    sources = [ "namei.c" ]
}

app_executable("fdlopen") {
    include_dirs = [ "include" ]
    package_name = "fdlopen"

    output_name = "fdlopen"

    sources = [ "fdlopen.c" ]
}

group("regtests") {
    deps = [ ":nameitests", ":fdlopen" ]
}
